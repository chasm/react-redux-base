{
  "name": "react-redux-base",
  "version": "1.0.0",
  "description": "A base on which to build React/Redux apps.",
  "main": "src/index.js",
  "scripts": {
    "watch": "watchify src/index.js -o public/js/app.js -v",
    "build": "browserify src/index.js -o public/js/app.js -v",
    "start": "http-server public -p 8008",
    "test": "mocha --opts ./test/mocha.opts test/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chasm/react-redux-base.git"
  },
  "keywords": [
    "chai",
    "cheerio",
    "enzyme",
    "jsdom",
    "mocha",
    "react",
    "react-router",
    "redux",
    "redux-router"
  ],
  "author": "Charles F. Munat <chas@munat.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chasm/react-redux-base/issues"
  },
  "homepage": "https://github.com/chasm/react-redux-base#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "http-server": "^0.8.5",
    "ramda": "^0.19.1",
    "react": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
    "jsdom": "^8.0.2",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3"
  }
}
