{
  "name": "react-redux-base",
  "version": "1.0.0",
  "description": "A base on which to build React/Redux apps.",
  "main": "src/index.js",
  "scripts": {
    "watch": "watchify src/index.js -o public/js/app.js -v",
    "build": "browserify src/index.js -o public/js/app.js -v",
    "start": "http-server public -p 8008",
    "test": "mocha --opts ./test/mocha.opts test/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chasm/react-redux-base.git"
  },
  "keywords": [
    "chai",
    "cheerio",
    "enzyme",
    "jsdom",
    "mocha",
    "react",
    "react-router",
    "redux",
    "redux-router"
  ],
  "author": "Charles F. Munat <chas@munat.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chasm/react-redux-base/issues"
  },
  "homepage": "https://github.com/chasm/react-redux-base#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "http-server": "^0.11.1",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^0.22.0",
    "enzyme": "^3.8.0",
    "jsdom": "^13.1.0",
    "mocha": "^5.2.0",
    "react-addons-test-utils": "^15.6.2"
  }
}
